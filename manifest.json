{
  "name": "La Quoc Thang",
  "description": "La Quốc Thắng",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "/logo/logo-16.png",
      "32": "/logo/logo-32.png",
      "48": "/logo/logo-48.png",
      "100": "/logo/logo-100.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/logo/logo-16.png",
    "32": "/logo/logo-32.png",
    "48": "/logo/logo-48.png",
    "100": "/logo/logo-100.png"
  },
  "permissions": [
    "scripting",
    "storage",
    "activeTab"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "background": {
    "service_worker": "lib/background.js"
  },
  "options_page": "options.html"
}